{
    "log_types": {
      "event": {
        "title": "Event Log",
        "fields": {
          "event_name": {
            "type": "string",
            "label": "Event Name",
            "required": true
          },
          "event_source": {
            "type": "string",
            "label": "Event Source",
            "required": true
          },
          "event_details": {
            "type": "string",
            "label": "Event Details",
            "required": false
          }
        }
      },
      "error": {
        "title": "Error Log",
        "fields": {
          "error_message": {
            "type": "string",
            "label": "Error Message",
            "required": true
          },
          "error_code": {
            "type": "integer",
            "label": "Error Code",
            "required": true
          },
          "stack_trace": {
            "type": "string",
            "label": "Stack Trace",
            "required": false
          }
        }
      },
      "haircut": {
        "title": "Haircut Log",
        "fields": {
          "date": {
            "label": "Date",
            "type": "date",
            "placeholder": "Select the date",
            "css_class": "form-control",
            "required": true
          },
          "shop": {
            "label": "Shop",
            "type": "text",
            "placeholder": "Enter the shop name",
            "css_class": "form-control",
            "required": true
          },
          "barber": {
            "label": "Barber",
            "type": "text",
            "placeholder": "Enter the barber's name",
            "css_class": "form-control",
            "required": true
          },
          "price": {
            "label": "Price",
            "type": "number",
            "placeholder": "Enter the price",
            "css_class": "form-control",
            "required": true
          }
        }
      }
    }
  }
  